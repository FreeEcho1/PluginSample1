<Window x:Class="PluginSample1.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PluginSample1"
        mc:Ignorable="d"
        Title="Settings" Height="200" Width="300">
    <Grid>
        <CheckBox x:Name="MoveSizeEndCheckBox" Content="MoveSizeEnd" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,-10,-10"/>
        <CheckBox x:Name="ShowCheckBox" Content="Show" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,32,-10,-32"/>
    </Grid>
</Window>
